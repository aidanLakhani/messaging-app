{
  "name": "messaging-app",
  "version": "1.0.0",
  "description": "Messaging app built by Aidan",
  "main": "server/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "cd server && node server.js",
    "client": "cd client && npm run dev",
    "server": "cd server && nodemon server.js",
    "dev": "concurrently -n 'server,client' -c bold.magenta,underline.cyan \"npm run server\" \"npm run client\""
  },
  "author": "Aidan Lakhani",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "sass": "^1.87.0"
  }
}
